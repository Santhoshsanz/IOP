{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "project": {
    "name": "iop"
  },
  "apps": [
    {
      "root": "src",
      "outDir": "dist",
      "assets": [
        "assets",
        "favicon.ico"
      ],
      "index": "index.html",
      "main": "main.ts",
      "polyfills": "polyfills.ts",
      "test": "test.ts",
      "tsconfig": "tsconfig.app.json",
      "testTsconfig": "tsconfig.spec.json",
      "prefix": "app",
      "styles": [
        "assets/stylesheets/custom/styles.css",
        "assets/icon-fonts/font-awesome.css",
        "assets/stylesheets/libs/ammap.css",
        "assets/stylesheets/libs/jquery.mCustomScrollbar.min.css",
        "assets/stylesheets/custom/main.css",
        "../node_modules/angular2-toaster/toaster.css"
      ],
      "scripts": [
        "assets/javascripts/libs/jquery-2.2.3.min.js",
        "assets/javascripts/libs/library-imports.js",
        "assets/javascripts/libs/imagesloaded.js",
        "assets/javascripts/libs/ammap.js",
        "assets/javascripts/libs/highcharts.js",
        "assets/javascripts/libs/exporting.js",
        "assets/javascripts/libs/heatmap.js",
        "assets/javascripts/libs/xrange.js",
        "assets/javascripts/libs/highcharts-more.js",
        "assets/javascripts/libs/solid-gauge.js",
        "assets/javascripts/libs/preloader.js",
        "assets/javascripts/libs/svgconvert.js",
        "assets/javascripts/libs/jquery-easing.js",
        "assets/javascripts/libs/countTo.js",
        "assets/javascripts/libs/jquery.mCustomScrollbar.min.js",
        "assets/javascripts/libs/device.min.js",
        "assets/javascripts/libs/shuffle.min.js",
        "assets/javascripts/libs/owl-carousel.min.js",
        "assets/javascripts/libs/maps/japanLow.js",
        "assets/javascripts/libs/maps/worldHigh.js",
        "assets/javascripts/libs/maps/worldLow.js",
        "assets/javascripts/custom/ammap-init.js",
        "assets/javascripts/custom/charts-init.js",
        "assets/javascripts/custom/main.js",
        "../node_modules/sockjs-client/dist/sockjs.min.js",
        "../node_modules/stompjs/lib/stomp.min.js"
      ],
      "environmentSource": "environments/environment.ts",
      "environments": {
        "dev": "environments/environment.ts",
        "prod": "environments/environment.prod.ts"
      }
    }
  ],
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "lint": [
    {
      "project": "src/tsconfig.app.json",
      "exclude": "**/node_modules/**"
    },
    {
      "project": "src/tsconfig.spec.json",
      "exclude": "**/node_modules/**"
    },
    {
      "project": "e2e/tsconfig.e2e.json",
      "exclude": "**/node_modules/**"
    }
  ],
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "defaults": {
    "styleExt": "css",
    "component": {}
  }
}
